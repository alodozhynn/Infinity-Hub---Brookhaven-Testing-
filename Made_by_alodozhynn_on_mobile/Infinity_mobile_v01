-- Adiciona o bot√£o "M√©todo 1, entrar dnv"
casasTab:AddButton({
    Name = "M√©todo 1, entrar dnv",
    Callback = function()
        -- Reentra no jogo no mesmo servidor
        local player = game.Players.LocalPlayer
        if player then
            -- Teleporta para o mesmo servidor
            local teleportService = game:GetService("TeleportService")
            teleportService:Teleport(game.PlaceId, player)
        end
    end
})

