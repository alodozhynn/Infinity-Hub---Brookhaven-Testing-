local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Window = OrionLib:MakeWindow({Name = "Infinity Hub Brookhaven [[beta]]", HidePremium = false, SaveConfig = true, ConfigFolder = "OrionTest"})

local mainTab = Window:MakeTab({
    Name = "Main/Trolling",
    Icon = "rbxassetid://4483345998",
    PremiumOnly = false
})

local adminTab = Window:MakeTab({
    Name = "Admin",
    Icon = "rbxassetid://4483345998",
    PremiumOnly = false
})

local teleportesTab = Window:MakeTab({
    Name = "Teleportes",
    Icon = "rbxassetid://4483345998",
    PremiumOnly = false
})

OrionLib:MakeNotification({
    Name = "Infinity Brookhaven",
    Content = "Made by Alodozhynn",
    Image = "rbxassetid://4483345998",
    Time = 5
})

mainTab:AddButton({
    Name = "Matar Jogador [precisa de um equipamento]",
    Callback = function()
        -- Teleporta o jogador para uma coordenada longe no eixo X
        rootPart.CFrame = CFrame.new(1000000, 0, 0)
    end    
})

mainTab:AddButton({
    Name = "Enviar pro Infinito",
    Callback = function()
        -- Teleporta o jogador para uma altura de 1.000.000
        rootPart.CFrame = CFrame.new(0, 1000000, 0)
    end    
})

