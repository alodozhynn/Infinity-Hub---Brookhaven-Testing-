veiculosTab:AddButton({
    Name = "Deletar carro",
    Callback = function()
        local player = game.Players.LocalPlayer
        local char = player.Character
        local hrp = char and char:FindFirstChild("HumanoidRootPart")

        if hrp and hrp.Parent and hrp.Parent:IsA("VehicleSeat") then
            -- Resetar o jogador se estiver sentado em um carro
            player.Character:BreakJoints()
        else
            OrionLib:MakeNotification({
                Name = "Voc√™ precisa entrar no carro",
                Content = "Voc√™ s√≥ pode remover um carro se entrar nele.",
                Image = "rbxassetid://4483345998",
                Time = 5
            })
        end
    end    
})

